        path(fill-rule='evenodd' clip-rule='evenodd' d='M2 12C2 11.4477 2.44772 11 3 11L21 11C21.5523 11 22 11.4477 22 12C22 12.5523 21.5523 13 21 13L3 13C2.44772 13 2 12.5523 2 12Z' fill='white')
    if (data.style)
      div(class='directions-item__block' style=`${data.style}`)._js-accord-block
        .directions-item__info
          .directions-item__left
          .directions-item__desc
            .directions-item__text=data.text
            a(href="javascript:void(0);").btn.btn-yellow.btn-arrow Подробнее
            .directions-item__name-big=data.name
    else
      div(class='directions-item__block')._js-accord-block
        .directions-item__info
          .directions-item__left
          .directions-item__desc
            .directions-item__text=data.text
            a(href="javascript:void(0);").btn.btn-yellow.btn-arrow Подробнее
            .directions-item__name-big=data.name

