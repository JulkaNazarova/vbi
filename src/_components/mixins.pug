mixin rating-row(num, position, title, year)
  .rating-row
    .row
      .col-4.col-md-4
        .rating-row__place
          .rating-row__num=num
          span место
      .col-8.col-md-4
        .rating-row__position=position
      .col-8.offset-4.col-md-4.offset-md-0
        .rating-row__title
          .rating-row__name=title
          .rating-row__year=year

            
mixin case-preview(data)
  a(href="javascript:void(0);" data-wow-delay=`${data.delay}ms`).case-preview__item.wow.animate__fadeInUp
    .sticker.case-preview__sticker=data.sticker
    .case-preview__img(style=`background-image:url('./img/${data.img}')`)
