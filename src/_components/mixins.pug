      .funnels-step__title !{title}
      if(text)
        .funnels-step__text !{text}
  else
    .funnels-step__item.wow.animate__fadeInUp(style=`width: ${width}%; background: ${bgcolor}`)
      .funnels-step__title !{title}
      if(text)
        .funnels-step__text !{text}

