mixin funnels-title(title, mod)
  if(mod)
    div(class='funnels-title ' + `${mod}`)
      .funnels-title__name=title
      button.btn.btn-primary.funnels-title__more(type="button" data-toggle="modal" data-target="#modalMoreInfo") 
        span Подробнее
      if(title == "Brandformance")
        +funnels-modal-lists-brandformance
      else if (title == "Performance")
        +funnels-modal-lists-performance
      else
        +funnels-modal-lists-other
  else
    .funnels-title
      .funnels-title__name=title
      button.btn.btn-primary.funnels-title__more(type="button" data-toggle="modal" data-target="#modalMoreInfo")
