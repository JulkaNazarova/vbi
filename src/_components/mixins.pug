mixin section-top(data)
  section.section-top
    .h1__wrap
      .container
        h1.h1.wow.animate__fadeInDown !{data.title}
    .page-head
      .container
        .row
          .col-12.col-md-4.col-lg-2.mb-3.mb-lg-0
            .page-slogan.wow.animate__fadeInLeft=data.slogan
          .col-lg-6.offset-lg-4
            .page-subtitle__wrap
              h2.h2.page-subtitle.wow.animate__fadeInUp !{data.subtitle}
              if(data.btn !== false)
                .page-head__buttons
                  a(href="#section-form").btn.btn-yellow.btn-arrow.scroll-to Обсудить проект
                

mixin page-banner(img)
  .page-banner
    img(src=`./img/${img}` alt='', title='')

mixin section-head(data)
  .section__head
    .container
      .row
        .col-lg-6
          .section__num.wow.animate__fadeInLeft=data.num
        .col-lg-6
          .section-h2__wrap
            if(data.title)
              h2.h2.wow.animate__fadeInUp=data.title
            if(data.colortext)
              .h2__color-text !{data.colortext}
            if(data.subtitle)
              .h2__subtitle !{data.subtitle}


mixin departments-item(data)
  .departments-item
    .departments-item__top
      a(href="javascript:void(0);").departments-item__title !{data.title}
      a(href="javascript:void(0);").departments-item__text=data.text
    .departments-item__bottom
      a(href="javascript:void(0);").sticker.departments-item__tag=data.tag1
      a(href="javascript:void(0);").sticker.departments-item__tag=data.tag2
      a(href="javascript:void(0);").sticker.departments-item__tag=data.tag3
    a(href="javascript:void(0);").departments-item__link
      span Подробнее
      .departments-item__btn

mixin directions-item(data)
  div(class='directions-item _js-accord-wrap ' + `${data.mod}`)
    .directions-item__bg.directions-bg_desktop(style=`background-image:url('./img/${data.desktop}')`)
    .directions-item__bg.directions-bg_tablet(style=`background-image:url('./img/${data.tablet}')`)
    .directions-item__bg.directions-bg_mobile(style=`background-image:url('./img/${data.mobile}')`)
    .directions-item__title._js-accord-title
      .directions-item__num-wrap
        .directions-item__num=data.num
        .directions-item__num-all 06
      .directions-item__name=data.name
    .directions-item__btn
      svg(class="directions-item__open" xmlns='http://www.w3.org/2000/svg' width='24' height='24' viewBox='0 0 24 24' fill='none')
        path(fill-rule='evenodd' clip-rule='evenodd' d='M13 4C13 3.44772 12.5523 3 12 3C11.4477 3 11 3.44771 11 4L11 11L4 11C3.44772 11 3 11.4477 3 12C3 12.5523 3.44772 13 4 13L11 13L11 20C11 20.5523 11.4477 21 12 21C12.5523 21 13 20.5523 13 20L13 13L20 13C20.5523 13 21 12.5523 21 12C21 11.4477 20.5523 11 20 11L13 11L13 4Z' fill='#1F1F1F')
